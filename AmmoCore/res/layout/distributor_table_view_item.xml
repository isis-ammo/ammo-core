<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content" android:padding="10px">
  
  <TextView android:id="@+id/distributor_table_view_item_uri"
  		android:layout_width="fill_parent"
  		android:layout_height="wrap_content"
  		android:padding="5px"
  		android:textSize="18px"
  		android:textColor="#FFF"/>
    
    <TextView android:id="@+id/distributor_table_view_item_disposition"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:padding="5px"
        android:textSize="18px"
        android:textColor="#FFF"/>
          		
  	<TextView android:id="@+id/distributor_table_view_item_timestamp"
  		android:layout_width="fill_parent"
  		android:layout_height="wrap_content"
        android:padding="5px"
        android:textSize="18px"
  		android:textColor="#FFF"/>
  	

    	
  	
  		
</LinearLayout>
